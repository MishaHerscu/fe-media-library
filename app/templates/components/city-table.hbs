<h2>Cities</h2>

<br /><br />

<table class="table table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Artists</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |city|}}
    <tr>
      <td>{{city.name}}</td>
      <td>Artists</td>
    </tr>
    {{/each}}
  </tbody>
</table>
