<table class="table table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>Founding Year</th>
      <th>Albums</th>
    </tr>
  </thead>
  <tbody>
    {{#each artists as |artist|}}
    <tr>
      <td>{{artist.name}}</td>
      <td>{{artist.based_in}}</td>
      <td>{{artist.founding_year}}</td>
      <td>Albums</td>
    </tr>
    {{/each}}
  </tbody>
</table>
